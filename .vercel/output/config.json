{
	"version": 3,
	"routes": [
		{
			"src": "/404/",
			"headers": {
				"Location": "/404"
			},
			"status": 308
		},
		{
			"src": "/about/",
			"headers": {
				"Location": "/about"
			},
			"status": 308
		},
		{
			"src": "/contact/",
			"headers": {
				"Location": "/contact"
			},
			"status": 308
		},
		{
			"src": "/projects/",
			"headers": {
				"Location": "/projects"
			},
			"status": 308
		},
		{
			"src": "/(?:\\/(.*?))?/([^/]+?)/(?:\\/(.*?))?/",
			"headers": {
				"Location": "/$1/$2/$3"
			},
			"status": 308
		},
		{
			"src": "/(?:\\/(.*?))?/([^/]+?)/(?:\\/(.*?))?/",
			"headers": {
				"Location": "/$1/$2/$3"
			},
			"status": 308
		},
		{
			"src": "/(?:\\/(.*?))?/(?:\\/(.*?))?/",
			"headers": {
				"Location": "/$1/$2"
			},
			"status": 308
		},
		{
			"src": "/(?:\\/(.*?))?/",
			"headers": {
				"Location": "/$1"
			},
			"status": 308
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404$",
			"dest": "_render"
		},
		{
			"src": "^\\/about$",
			"dest": "_render"
		},
		{
			"src": "^\\/contact$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}